{{#if alreadyEnrolled}}
  <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
  <script>
    Swal.fire({
      icon: 'info',
      title: 'Bạn đã đăng ký!',
      text: 'Tất cả các khóa học trong giỏ hàng của bạn đã được đăng ký.',
      confirmButtonText: 'OK'
    }).then(() => {
      window.location.href = '/account/cart';
    });
  </script>
{{/if}}
